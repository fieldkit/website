{
  "dependencies": {
    "jquery": "^3.7.1",
    "lazysizes": "^5.3.2",
    "magnific-popup": "^1.2.0",
    "normalize.css": "^8.0.1",
    "sass-mq": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "cssnano": "^7.0.6",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "postcss-cli": "^11.0.0",
    "postcss-custom-media": "^11.0.5",
    "prettier": "^3.5.1",
    "standard-version": "^9.5.0",
    "stylelint": "^16.14.1",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.11.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": "22.14.0"
  },
  "license": "GPL-3.0-or-later",
  "private": true,
  "scripts": {
    "build": "npm run build:scripts && npm run build:styles",
    "build:scripts": "webpack --mode=production",
    "build:styles": "sass --load-path=node_modules fieldkit/assets/styles/main.scss fieldkit/style.css",
    "commit": "git-cz",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint \"**/*.scss\"",
    "postbuild:styles": "postcss fieldkit/style.css -o fieldkit/style.css",
    "release": "standard-version",
    "watch": "npm run watch:scripts & npm run watch:styles",
    "watch:scripts": "webpack --mode=development --watch",
    "watch:styles": "sass --load-path=node_modules --watch fieldkit/assets/styles/main.scss fieldkit/style.css",
    "prepare": "husky"
  },
  "version": "0.1.7"
}
