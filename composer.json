{
  "config": {
    "sort-packages": true
  },
  "extra": {
    "installer-paths": {
      "wordpress/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/junaidbhura/composer-wp-pro-plugins"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?a=download&p=pro&k={%ACF_PRO_KEY}"
        },
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^5.0"
        },
        "type": "wordpress-plugin",
        "version": "5.8.12"
      },
      "type": "package"
    }
  ],
  "require": {
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/acf-content-analysis-for-yoast-seo": "^2.3",
    "wpackagist-plugin/adminimize": "^1.11",
    "wpackagist-plugin/advanced-custom-fields-table-field": "^1.3",
    "wpackagist-plugin/akismet": "^4.1",
    "wpackagist-plugin/cf7-conditional-fields": "^1.7",
    "wpackagist-plugin/classic-editor": "^1.5",
    "wpackagist-plugin/contact-form-7": "^5.2",
    "wpackagist-plugin/duplicate-post": "^3.2",
    "wpackagist-plugin/flamingo": "^2.1",
    "wpackagist-plugin/jetpack": "^8.5",
    "wpackagist-plugin/redirection": "^4.3",
    "wpackagist-plugin/w3-total-cache": "^2.2.9",
    "wpackagist-plugin/woocommerce": "^4.1",
    "wpackagist-plugin/woocommerce-gateway-paypal-express-checkout": "^2.0",
    "wpackagist-plugin/woocommerce-gateway-stripe": "^4.3",
    "wpackagist-plugin/woocommerce-services": "^1.23",
    "wpackagist-plugin/woocommerce-shipstation-integration": "^4.1",
    "wpackagist-plugin/wordfence": "^7.4",
    "wpackagist-plugin/wordpress-seo": "^14.0",
    "wpackagist-plugin/wp-ses": "^1.3",
    "wpackagist-plugin/wp-smushit": "^3.6"
  }
}
